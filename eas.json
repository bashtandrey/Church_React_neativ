{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "distribution": "internal",
        "cache": {
          "disabled": true
        },
        "autoIncrement": true
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "ios": {
        "distribution": "store",
        "cache": {
          "disabled": true
        },
        "autoIncrement": true
      }
    }
  }
}